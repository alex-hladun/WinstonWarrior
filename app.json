{
  "expo": {
    "name": "Winston Warrior",
    "slug": "WinstonWarrior2",
    "version": "1.0.0",
    "privacy": "public",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "primaryColor": "#99D969",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#99D969"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "buildNumber": "1.0.0",
      "bundleIdentifier": "com.alexhladun.winstonwarrior",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses your location to measure distance to the pin."
      }
    },
    "android": {
      "package": "com.alexhladun.winstorwarrior",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "description": "Golf Tracker",
    "githubUrl": "https://github.com/alex-hladun/WinstonWarrior"
  }
}
